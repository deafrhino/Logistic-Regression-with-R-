library(aod)
library(ggplot2)
install.packages("rms")
require(rms)
require(foreign)
require(nnet)
require(ggplot2)
require(reshape2)
mydata <- read.csv("c://Users/siddi/Documents/new_logit.csv", header = T, colClasses = "factor")
str(mydata)
sapply(mydata, sd)
sapply(data, sd)
mylogit <- glm(Empolyment.Status ~ Gender + Race +  Age.Group + Province + Education.Status + Marital.Status + Long.term.unemployment , data = mydata ,family = "binomial")
summary(mylogit)
confint(mylogit)
fit <- glm(Attendance ~ Province + Gender + Age.Group + Race + Tuition.Fees.Paid + Social.Grants, data = mydata,family = binomial("logit"), maxit = 100)
summary(fit)
confint(fit)
l <- cbind(0, 0, 0, 1, -1, 0)
wald.test(b = coef(mylogit), Sigma = vcov(mylogit), L = l)
wald.test(b = coef(mylogit), Sigma = vcov(mylogit), Terms = 4:6)
summary(mpg.mod)
mod1b <- lrm(y ~ x)
mod1b <- lrm(Employment.Status ~ x)
anova(model, test="Chisq")
anova(mylogit, test="Chisq")
